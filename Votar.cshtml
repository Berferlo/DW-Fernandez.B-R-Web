@{
    var id=Request.QueryString["id"];
    var idi=Convert.ToInt32(id);

	if(idi==1)
    {
        if(@WebSecurity.IsAuthenticated)
        {
            Response.Redirect("~/secrgen");
        }
        else
        {
            Response.Redirect("~/login");
        }
    }
    else if(idi==2)
    {
        if(@WebSecurity.IsAuthenticated)
        {
            Response.Redirect("~/subsgen");
        }
        else
        {
            Response.Redirect("~/login");
        }
    }
    else if(idi==3)
    {
        if(@WebSecurity.IsAuthenticated)
        {
            Response.Redirect("~/comgar");
        }
        else
        {
            Response.Redirect("~/login");
        }
    }
    else
    {
        if(@WebSecurity.IsAuthenticated)
        {
            Response.Redirect("~/consciu");
        }
        else
        {
            Response.Redirect("~/login");
        }
    }
}

